<h2>ジャンル編集</h2>

<div class="row">
  <div class="col-md-11 offset-md-1">
    <%= form_with model:@genre, url: admin_genre_path(@genre), local:true do |f| %>
      <p class="d-inline-block">ジャンル名</p>
      <%= f.text_field :name, class: "ml-3" %>
    　<%= f.submit'変更を保存', class: "btn outline btn-success" %>
    <% end %>
  </div>
 </div>

